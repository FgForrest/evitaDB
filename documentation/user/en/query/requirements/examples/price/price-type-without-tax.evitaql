query(
    collection('Product'),
    filterBy(
        priceInPriceLists('basic'),
        priceInCurrency('EUR'),
        priceBetween(100, 103)
    ),
    require(
      priceType(WITHOUT_TAX),
      entityFetch(
        attributeContent('code'),
        priceContentRespectingFilter()
      )
    )
)